{
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "node": true,
        "browser": true,
        "es6": true,
        "mocha": true
    },
    "options": { "treatUndefinedAsUnspecified": false },
    "rules":{
        "linebreak-style": 0,
        "comma-dangle": "warn",
        "no-unused-vars": [
            "error",
            {
                "vars": "local",
                "args": "none"
            }
        ]
      }
    
}